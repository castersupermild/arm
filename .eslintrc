{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "airbnb",
    "plugin:vue/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "es5"
    }]
  }
}
